{
  "name": "agent-loadout",
  "version": "1.0.2",
  "description": "One command to load out your terminal for agentic coding",
  "type": "module",
  "bin": {
    "agent-loadout": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "brewfile": "tsx scripts/generate-brewfile.ts",
    "prepack": "pnpm build && pnpm typecheck",
    "version": "pnpm build && git add -A dist",
    "test": "tsx --test src/**/*.test.ts"
  },
  "keywords": [
    "cli",
    "ai",
    "agent",
    "developer-tools",
    "loadout"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^8.3.0",
    "chalk": "^5.4.0",
    "commander": "^14.0.3",
    "execa": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
